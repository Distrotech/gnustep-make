#
# Binary and compile tools
#
CC = @CC@

EXEEXT = 
OEXT = .o
LIBEXT = .a
RESEXT = .res
RCEXT = .rc

LN_S = @LN_S@

LD = gcc
LDOUT =
LDFLAGS = @LDFLAGS@ -o

AR = ar
AROUT =
ARFLAGS = rc
RANLIB = @RANLIB@

RC = @RC@
DLLTOOL = @DLLTOOL@

# The default library combination
library_combo = @ac_cv_library_combo@

ifeq ($(GNUSTEP_TARGET_OS),nextstep4)
library_combo = nx_nx_nx_nil
endif

#
# X Window System headers and libraries
#
X_INCLUDE := @X_INCLUDE@
X_LIBS := @X_LIBS@
