  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  if test -n "$GNUMAKE"; then
  ac_cv_prog_GNUMAKE="$GNUMAKE" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
